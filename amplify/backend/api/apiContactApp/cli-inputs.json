{
  "version": 1,
  "paths": {
    "/contacts": {
      "name": "/contacts",
      "lambdaFunction": "crudContactApp",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/labels": {
      "name": "/labels",
      "lambdaFunction": "labelCRUD",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}